-------------------------------------------------------------------
-- Tracing functions for powersamba
-------------------------------------------------------------------
rEntern(u: Xn CX, ab: QXABn CX): Void == (_
    vPrint("-- -----------------------------------------------red u", qetaGrades u);_
    vPrint("-- ---------------------------red lc u", [qetaLeadingCoefficient(u, k) for k in 1..maxIndex()$Xn(CX)])_
    );
-------------------------------------------------------------------
rLoopn(u: Xn CX): Void == (_
    vPrint("-- red loop", qetaGrades u));
-------------------------------------------------------------------
rReturnn(u: Xn CX): Void == vPrint("-- redx", first u);
-------------------------------------------------------------------
Yn ==> QXCOMPn CX;
SXn ==> Record(sgi: List List Z, elem: Xn CX);
XXn ==> Record(sgi: List List Z, xf1: Xn CX, xf2: Xn CX);
Products ==> XHashTable(Z, List XXn);
YnRep ==> Record(ab: QXABn CX, cbe: List SXn, pairs: Products);
yyn ==> y pretend YnRep;
-------------------------------------------------------------------
ytracen(y: Yn): Void == (_
    vPrint("-- ==================================step(#cbe,#pairs)",_
           [#(yyn.cbe), #(yyn.pairs)]$List(Z))_
    )
-- noTrace(x: Yn): Void == void()
-------------------------------------------------------------------
-- onetnStep!(y: Yn): Yn == (oneTracedStep!$Yn)(noTrace, rEntern, rLoopn, rReturnn)(y)
onetnStep!(y: Yn): Yn == (oneTracedStep!$Yn)(ytracen, rEntern, rLoopn, rReturnn)(y)

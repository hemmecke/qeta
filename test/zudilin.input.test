findEquivalentMatrix(mat: MZ, mats: List MZ): List Z == (_
    res: List Z := empty();_
    imat := IMAT mat;_
    for m in mats for i in 1.. repeat (_
        if zero? positiveRemainder((m * imat)(2,1), level) then (_
            res := cons(i, res))_
    );_
    res);


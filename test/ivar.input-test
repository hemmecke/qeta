--setup
P ==> PositiveInteger
Z ==> Integer
V ==> IndexedVariable("f")
--endsetup

--test:ivar
assertTrue(zero?(rank()$V))
f1 := new()$V
assertEquals(lookup f1, 1)
assertTrue(one?(rank()$V))
f2 := new()$V
assertEquals(lookup f1, 1)
assertEquals(lookup f2, 2)
assertEquals(rank()$V, 2)
v1 := index(1)$V
assertEquals(v1, f1)
v2 := index(2)$V
assertEquals(v2, f2)
assertTrue(f1 < f2)
s1: Symbol := "f1"::Symbol
assertEquals(convert(f1)@Symbol, s1)
s2: Symbol := "f2"::Symbol
assertEquals(convert(f2)@Symbol, s2)
--endtest

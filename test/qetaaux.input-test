pp(a,p,e) ==> assertEquals(primePower(a, p), e)
pp(5, 3, 0)
pp(6, 3, 1)
pp(12, 3, 1)
pp(9, 3, 2)
pp(12, 2, 2)
pp(36, 5, 0)
pp(72, 2, 3)
pp(72, 3, 2)

-- +
--endtest



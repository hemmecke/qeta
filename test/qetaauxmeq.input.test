cog0(3, [1, 1/3])
cog0(4, [1, 1/2, 1/4])
cog0(5, [1, 1/5])
cog0(6, [1, 1/2, 1/3, 1/6])
cog0(7, [1, 1/7])
cog0(8, [1, 1/2, 1/4, 1/8])
cog0(9, [1, 1/3, 2/3, 1/9])
cog0(10, [1, 1/2, 1/5, 1/10])
cog0(11, [1, 1/11])
cog0(12, [1, 1/2, 1/3, 1/4, 1/6, 1/12])

--test:integer
-- # The relation in $M_i$'s with integer coefficients
--
-- Here $\mathbb{Z}$ is considered as a Euclidean domain wrt the
-- absolute value as the Euclidean size function.
XZ ==> QEtaExtendedAlgebra(Z, LZ, PolZ)
divs := divisors(level)$IntegerNumberTheoryFunctions;
rgens := xetaQuotientMonoidExponentVectors level;
etaquotgensZ: List EtaQuotient(Z, LZ) := [etaQuotient(divs, r) for r in rgens];
eqgensZ: List LZ := [retract x for x in etaquotgensZ];
msyms := indexedSymbols("M", #rgens)
xeqgensZ: List XZ := [xembed(x, s) for x in eqgensZ for s in msyms]
tez := xeqgensZ.1
abez := algebraBasis(tez, xeqgensZ)$QEtaAlgebraBasis(Z, XZ, QEtaExtendedComputation(Z, LZ, XZ))
assertEquals(#abez, 5)

pfZ := retract(q24(13)$EtaQuotient(Z, LZ) *_
       etaQuotient(divs, [10,2,11,-22])$EtaQuotient(Z, LZ));
ps: LZ := partitionSeries(1)$QFunctions(Z, LZ)
r11: LZ := choose((n: Z): Z +-> 11*n+6, ps, 0)
fe := pfZ * r11
fez: XZ := xembed(fe, F)

-- Interestingly, we have to multiply by 4, because otherwise the
-- reduction would not return 0.
rez := reduce(4*fez, tez, abez)
assertTrue(zero? rez)

rz := second rez;
rzExpected: PolZ := _
  139799528*M6+(-19800*M1-718078856)*M5_
  +(176*M1^2+18031904*M1-16752297419)*M4+(-51656*M1+3007344648)*M3_
  +(-132*M1^2-13523928*M1+4586709248)*M2-44*M1^3-4903712*M1^2_
  -593090333*M1+4*F-18978271136;
assertEquals(rz, rzExpected)

mrez := monomials rz;
exp11 := [exponent(leadingCoefficient(x)::C) for x in mrez]
exp11Expected := [2, 1, 2, 1, 2, 3, 1, 2, 1, 2, 2, 1, 1, 2, 0, 3];
assertEquals(exp11, exp11Expected)

ez := eval(rz, c+->c::LZ, cons('F,msyms), cons(first fez,eqgensZ))_
      $ PolynomialEvaluation(Z, LZ)
assertTrue(zero? ez)
--endtest


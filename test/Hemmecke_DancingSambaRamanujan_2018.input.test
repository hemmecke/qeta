denoms := [unitCanonical(denom(x)::C) for x in monomials rq]
terms := [((inv(leadingCoefficient(x)))*x)::Pol for x in monomials rq]
monoms := [d*t for d in denoms for t in terms]
monomsExpected: List Pol := [_

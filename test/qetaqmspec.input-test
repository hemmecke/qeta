-- This file tests functionality from qetaqmev.spad.

--setup
QETAMEV  ==> QEtaQuotientMonoidExponentVectors
QES4TI2 ==> QEtaQuotientSpecifications4ti2
eqmev ==> etaQuotientMonoidExponentVectors $ QETAMEV
eqmspecs ==> etaQuotientMonoidSpecifications $ QES4TI2
eqspec ==> etaQuotientSpecification $ QEtaSpecification
ae(n, result) ==> (
  assertEquals(eqmev n, result);_
  assertEquals(eqmspecs n, [eqspec(n,x) for x in result]))
aespec(n) ==> assertEquals([eqspec(n,x) for x in eqmev n], eqmspecs n)
--endsetup

--test:v0
ae(2, [[24, -24]])
ae(3, [[12, -12]])
ae(4, [[- 8, 24, - 16], [8, 0, - 8]])
ae(5, [[6, -6]])
ae(6, [[- 3, 3, 9, - 9], [- 4, 8, 4, - 8], [5, - 1, 1, - 5]])
ae(7, [[4, -4]])
ae(8, [[0, - 4, 12, - 8], [- 4, 10, - 2, - 4], [4, - 2, 2, - 4]])
ae(9, [[3, 0, - 3], [- 3, 12, - 9]])
ae(10,[[- 1, 1, 5, - 5], [- 2, 4, 2, - 4], [3, - 1, 1, - 3]])
--endtest

--test:v1
for n in 11..19 repeat aespec(n)
--endtest

--test:v2
for n in 20..29 repeat aespec(n)
--endtest

--test:v3
-- Computation qemev(30) consumes too much space.
for n in 31..39 repeat aespec(n)
--endtest

--test:v4
-- eqmev consumes too much time/space for 40 and 42
for n in [41,43,44,45,46,47,49] repeat aespec(n)
--endtest

